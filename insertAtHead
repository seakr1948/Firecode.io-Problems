public ListNode insertAtHead(ListNode head, int data) {

            ListNode newNode = new ListNode(data);
            
  if (head == null){ // if head is null, set newNode as new head
      return newNode;
  }          
else 
{ // sets the head as the next node, making newNode the head node
    newNode.next = head;
    
    return newNode;
}

}
